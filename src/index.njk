---
layout: default
title: Leaderboard
---
<h1>Leaderboard</h1>

<div class="form-outline mb-3">
    <div id="spinner" class="spinner-border text-muted mr-2" role="status">
        <span class="visually-hidden">Loading...</span>
    </div>
    <label for="group">Group</label>
    <input type="number" id="group" min="1" max="100"> (of <span id="max"></span>)
</div>

<table class="table">
    <thead>
    <tr>
        <th scope="col">#</th>
        <th scope="col">Name</th>
        <th scope="col">Score</th>
    </tr>
    </thead>
    <tbody id="leaderboard">

    </tbody>
</table>

<script src="/assets/js/leaderboard.min.js"></script>
